        <messages/>
        <name>Calculate Seats Percentage (onChange Ma</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   // Get the values of the maximum seats and available seats fields
   var maxSeats = g_form.getIntValue('maximum_seats');
   var availableSeats = g_form.getIntValue('available_seats');

   // Check if maxSeats is a valid, non-zero number before calculating
   if (maxSeats > 0) {
      // Calculate the percentage filled: ((Max - Available) / Max) * 100
      var seatsFilled = maxSeats - availableSeats;
      var percentage = (seatsFilled / maxSeats) * 100;
      
      // Round the percentage to two decimal places
      var roundedPercentage = percentage.toFixed(2);

      // Set the value of the 'Seats Percentage Filled' field
      // Replace 'seats_percentage_filled' with your actual backend field name
      g_form.setValue('filled_seats_percentage', roundedPercentage);
   } else {
      // If max seats is 0 or invalid, clear the percentage field or set to 0
      g_form.setValue('filled_seats_percentage', '');
   }
}
]]></script>
