        <script><![CDATA[(function executeRule(current, previous) {

   var maxSeats = parseInt(current.maximum_seats, 10);
   var availableSeats = parseInt(current.available_seats, 10);

   if (!isNaN(maxSeats) && maxSeats > 0) {

      var seatsFilled = maxSeats - availableSeats;

      if (seatsFilled < 0) {
         seatsFilled = 0;
      }
      if (seatsFilled > maxSeats) {
         seatsFilled = maxSeats;
      }

      var percentage = (seatsFilled / maxSeats) * 100;
      current.filled_seats_percentage = Math.round(percentage * 100) / 100;

   } else {
      current.filled_seats_percentage = '';
   }

})(current, previous);
]]></script>
